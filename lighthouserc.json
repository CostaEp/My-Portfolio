{
    "ci": {
      "collect": {
        "staticDistDir": "./build",
        "startServerCommand": "npx serve ./build",
        "startServerReadyPattern": "Accepting connections"
      },
      "assert": {
        "preset": "lighthouse:recommended"
      },
      "upload": {
        "target": "temporary-public-storage"
      }
    }
  }